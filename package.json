{
  "name": "angular_template",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./bin/www",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node ./bin/www",
    "build": "ng build && workbox generate:sw",
    "build-prod": "ng build --prod --aot && workbox generate:sw",
    "build-prod-analysis": "ng build --prod --aot --stats-json && workbox generate:sw",
    "create-sw": "workbox generate:sw",
    "test": "ng test --sourcemaps=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "static-serve": "cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "pm2-start": "pm2 start ./bin/www --node-args='--max_old_space_size=4096' --name='Angular Template' --watch"
  },
  "private": true,
